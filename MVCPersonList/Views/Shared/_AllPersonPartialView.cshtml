@model List<Person>


<!-- -------------------------------------------------- Table ----------------------------------- -->
<div class="row">
    <div class="col-12">
        <!-- ------------------------------------------ Head ------------------------------------- -->
        <div class="row">
            <!-- Name -->
            <div class="col-4">
                <h5>Name</h5>
            </div>
            <!-- City -->
            <div class="col-4">
                <h5>City</h5>
            </div>
            <!-- Phone -->
            <div class="col-2">
                <h5>Phone</h5>
            </div>
            <!-- Action -->
            <div class="col-2">
                <h5>Action</h5>
            </div>
        </div>


        <!-- --------------------------------------- Body ---------------------------------- -->

        @foreach (var item in Model)
        {
            <partial name="_APersonRowPartialView" model="item" />
           
        }
    </div>
</div>
